{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "demobitsaug2022"
		},
		"LS_SQL_SourceDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_SourceDB'"
		},
		"LS_SQL_TargetDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_TargetDB'"
		},
		"LS_Storage_demobits_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Storage_demobits'"
		},
		"TRG_PL_CopyFile_Dynamic_properties_PL_CopyFile_Dynamic_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"TRG_PL_CopyFile_Dynamic_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d003678d-3be9-4518-b21e-a5101bea6844/resourceGroups/demoRG/providers/Microsoft.Storage/storageAccounts/demobits"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "demobitsaug2022_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQL_SourceDB_connectionString": {
						"value": "[parameters('LS_SQL_SourceDB_connectionString')]"
					},
					"LS_SQL_TargetDB_connectionString": {
						"value": "[parameters('LS_SQL_TargetDB_connectionString')]"
					},
					"LS_Storage_demobits_connectionString": {
						"value": "[parameters('LS_Storage_demobits_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "demobitsaug2022_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TRG_PL_CopyFile_Dynamic_properties_PL_CopyFile_Dynamic_parameters_FileName": {
						"value": "[parameters('TRG_PL_CopyFile_Dynamic_properties_PL_CopyFile_Dynamic_parameters_FileName')]"
					},
					"TRG_PL_CopyFile_Dynamic_properties_typeProperties_scope": {
						"value": "[parameters('TRG_PL_CopyFile_Dynamic_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'demobitsaug2022_ArmTemplate_0')]"
			]
		}
	]
}